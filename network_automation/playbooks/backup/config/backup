    config wireless-controller utm-profile
        edit wifi-default
          set comment "Default configuration for offloading WiFi traffic."
          set webfilter-profile "wifi-default"
          set application-list "wifi-default"
          set antivirus-profile "wifi-default"
          set ips-sensor "wifi-default"
        next
    end
    config log memory setting
      set status enable
    end
    config log disk setting
      set status enable
    end
    config log null-device setting
      set status disable
    end
    config router rip
        config redistribute connected
        end
        config redistribute static
        end
        config redistribute ospf
        end
        config redistribute bgp
        end
        config redistribute isis
        end
    end
    config router ripng
        config redistribute connected
        end
        config redistribute static
        end
        config redistribute ospf
        end
        config redistribute bgp
        end
        config redistribute isis
        end
    end
    config router static
        edit 1
          set device "TRANSIT 101"
          set gateway 172.16.101.1
        next
    end
    config router ospf
      set router-id 192.168.1.31
        config area
            edit 1.1.1.1
            next
        end
        config ospf-interface
            edit InternetFw
              set interface "port1"
              set dead-interval 40
              set hello-interval 10
            next
        end
        config network
            edit 1
              set prefix 10.0.10.0 255.255.255.0
              set area 1.1.1.1
            next
        end
        config redistribute connected
        end
        config redistribute static
        end
        config redistribute rip
        end
        config redistribute bgp
        end
        config redistribute isis
        end
    end
