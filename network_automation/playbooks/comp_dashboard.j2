                        <thead>
                            <tr>
                                <th>HOST</th>
                                <th>TECHNOLOGIE</th>
                                <th>COMPLIANCY</th>
                            </tr>
                        </thead>
                        <tbody>
                        {% for comp in cumulus_comp['list'] %}
                            <tr>
                              <td>"{{comp['host']}}"</td>
                              <td>"{{comp['type']}}"</td>
                              <td>"{{comp['result']}}"</td>

                            </tr>
                        {% endfor  %}
                        </tbody>
                    </table>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
